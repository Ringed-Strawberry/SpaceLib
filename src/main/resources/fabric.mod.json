{
  "schemaVersion": 1,
  "id": "spacelib",
  "version": "${version}",
  "name": "SpaceLib",
  "description": "Adds customizable Tool & Equipment, tailored by each player, for each player!",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/spacelib/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "ringed_strawberry.github.io.spacelib.client.SpacelibDataGenerator"
    ],
    "client": [
      "ringed_strawberry.github.io.spacelib.client.SpacelibClient"
    ],
    "main": [
      "ringed_strawberry.github.io.spacelib.Spacelib"
    ]
  },
  "mixins": [
    "spacelib.mixins.json",
    {
      "config": "spacelib.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
